{
  "name": "hull-connector",
  "description": "Hull Archetype for Connector development (Uses Builder)",
  "version": "1.0.6",
  "main": "dist/index.js",
  "license": "MIT",
  "homepage": "https://hull.io/docs",
  "keywords": ["hull", "ship", "hull-ship"],
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "8.6.0",
    "npm": "3.x",
    "yarn": "1.2.1"
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json}": ["prettier --write --single-quote", "git add"]
  },
  "scripts": {
    "gen-dev": "./node_modules/.bin/builder-support gen-dev",
    "publish:both": "npm publish ; npm publish dev",
    "lint": "eslint --color -c config/eslint/.eslintrc src server",
    "flow": "flow",
    "clean": "rimraf dist; rimraf lib",
    "update": "npm-check",
    "precommit": "lint-staged",
    "prepare": "NODE_ENV=production babel lib --copy-files -d dist",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\" --write \"server/**/*.js\"",
    "build": "builder run clean && builder run build:server && builder run build:client",
    "build:server": "NODE_ENV=production npx babel server --copy-files -d lib",
    "build:client": "NODE_ENV=production npx webpack --config ./node_modules/hull-connector/config/webpack.config.js",
    "dev": "NODE_ENV=development babel-watch -w server -w src server/index.js",
    "npm:test": "builder run test:lint && builder run test:units && builder run test:specs",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "test:coverage": "NODE_ENV=test jest --env=jsdom --config=jest.config.json --coverage",
    "test:client": "NODE_ENV=test jest --env=jsdom --config=jest.config.json",
    "test:units": "NODE_ENV=test mocha --require babel-register -R spec ./tests/unit/*.js",
    "test:specs": "NODE_ENV=test mocha --require babel-register -R spec ./tests/integration/*.js"
  },
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-replace-require": "0.0.4",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "builder": "^3.2.3",
    "builder-support": "^0.5.0",
    "chalk": "^2.3.0",
    "css-loader": "^0.28.7",
    "glob": "^7.1.2",
    "happypack": "^4.0.0",
    "lint-staged": "^4.3.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-flexbugs-fixes": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-alternate-require-loader": "^0.0.3"
  },
  "devDependencies": {}
}

{
  "name": "hull-connector",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://hull.io/docs",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "8.6.0",
    "npm": "3.x",
    "yarn": "1.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json}": ["prettier --write --single-quote", "git add"]
  },
  "scripts": {
    "lint": "eslint src",
    "flow": "flow",
    "ngrok": "ngrok http 8082 --region eu --subdomain hullconnector",
    "nodemon":
      "NODE_ENV=development nodemon server/index.js --exec babel-node --watch server",
    "precommit": "lint-staged",
    "clean": "rimraf dist; rimraf lib",
    "update": "updtr",
    "build": "yarn clean && yarn build:server && yarn build:client",
    "build:client":
      "NODE_ENV=production webpack --config ./webpack.config.js --progress --profile --colors -p",
    "build:server": "NODE_ENV=production babel server --copy-files -d lib",
    "start": "NODE_ENV=production node -r newrelic ./lib",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug yarn run nodemon",
    "test": "yarn test:lint && yarn test:units && yarn test:specs",
    "test:lint": "eslint server",
    "test:modules": "npm outdated --depth=0",
    "test:coverage":
      "NODE_ENV=test jest --env=jsdom --config=jest.config.json --coverage",
    "test:client": "NODE_ENV=test jest --env=jsdom --config=jest.config.json",
    "test:units":
      "NODE_ENV=test mocha --require babel-register -R spec ./tests/unit/*.js",
    "test:specs":
      "NODE_ENV=test mocha --require babel-register -R spec ./tests/integration/*.js"
  },
  "keywords": ["hull", "ship", "hull-ship"],
  "dependencies": {
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "hull": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "newrelic": "^2.2.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "raven": "^2.2.1",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0",
    "sass-loader": "^6.0.6"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-replace-require": "0.0.4",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.6",
    "builder": "^3.2.3",
    "builder-support": "^0.5.0",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.48.0",
    "happypack": "^4.0.0",
    "husky": "^0.14.2",
    "lint-staged": "^4.0.0",
    "postcss-flexbugs-fixes": "^3.0.0",
    "prettier": "^1.5.2",
    "progress-bar-webpack-plugin": "^1.10.0",
    "prop-types": "^15.5.10",
    "react-redux": "^5.0.6",
    "reactotron-react-js": "^1.12.2",
    "reactotron-redux": "^1.12.2",
    "redux-promise": "^0.5.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "styled-components": "^2.2.1",
    "svg-inline-loader": "^0.8.0",
    "updtr": "^2.0.0",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
